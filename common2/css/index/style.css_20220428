/*=== parts.css header ===*/

#main h2{
	font-size:32px;
	font-weight:700;
	font-family: Linotte, YuGothic, "Yu Gothic", 游ゴシック体, 游ゴシック, "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", Roboto, メイリオ, Meiryo, "ＭＳ Ｐゴシック", Osaka, "MS PGothic", Arial, Helvetica, Verdana, sans-serif;
}
#main h3{
	font-size:24px;
}
#main h4{
	font-size:18px;
	font-weight:400;
}

body header{
    height: 165px!important;
    width: 100% !important;
    margin-bottom: 0 !important;
	box-shadow: 0px 3px 7px 3px #dedede;
    -moz-box-shadow: 0px 3px 7px 3px #dedede;
}

.header_top{
	display:flex;
	justify-content:space-between;
	margin:0 0 20px;
	font-size:16px;
	font-weight:700;
	color:#e75f9d;
	line-height:1.6em; 
}
.header_top h1.headLogo a {
    background: url(/common2/img/common/headTtl.png) no-repeat 0 0;
    width: 150px;
    height: 50px;
    background-size: 150px;
    display: block;
	text-indent:100%;
	white-space:nowrap;
	overflow:hidden;
}
.header_top h2{
	font-size:2.0em;
	color:#b50081;
}
.header_top h2 img{
	margin:0 8px 0 0;
}
/* nav */
.nav_fixed{
	position:fixed;
	width:100%;
	height: 143px;
	padding:20px 0 0;
	background-color:#fff;
	box-shadow: 0px 1px 3px 1px #E3E3E3;
    -moz-box-shadow: 0px 1px 3px 1px #E3E3E3;
	z-index:9999;
}
.smpCtl{
	display: none;
}
.gnav{
	display:flex;
}
.gnav li{
	border-left:1px solid #b70081;
	width:calc(100%/6);
	text-align:center;
	vertical-align:middle;
	display:block;
}
.gnav li:first-child{
	border-left:none;
}
.gnav li a {
	text-decoration:none;
	color:#5e2f16;
	font-size:16px;
	font-weight:700;
	line-height:1.4em;
    display: block;
    padding: 10px 0;
}
.gnav li.gnav_staff a{
	text-decoration:none;
	color:#5e2f16;
	font-size:16px;
	font-weight:700;
	line-height:1.4em;
    display: block;
    padding:0;
}
.gnav li a:hover {
	color:#b50081;
}

/* MV */
.mv_area{
	width:100%;
	background-color:#fcf3fc;
	background-image:linear-gradient(-90deg,#fff 30%,transparent 30%);
	background-size:8px 8px;
	padding:20px 0 100px;
}
.mv_inner{
	display:flex;
}
.mv_box_l{
	width:660px;
	margin:0 10px 0 80px;
	position:relative;
}
.mv_slider_frame{
	z-index:999;
}
.mv_kon_ani{
	position:absolute;
	bottom: -5px;
    left: -90px;
	width:150px;
	height:119px;
	z-index:12;
}
/* MV-slider */
.swiper-container,.swiper-container img{
	width:520px;
	height:347px;
	overflow:hidden;
}
.swiper-container{
	position:absolute;
	top:35px;
	left:70px;
	z-index:10;
}
.swiper-button-next.white, .swiper-rtl .swiper-button-prev {
    right:20px;
    left: auto;
}
.swiper-button-next.white, .swiper-button-prev.white {
	top:46%;
}
.swiper-button-next.white:after, .swiper-button-prev.white:after {
	color:#FFF;
	display:block;
	transform:scale(0.6);
}
.swiper-pagination{
	bottom: 20px;
	left:40%;
}
.swiper-pagination-bullet{
	margin:0 5px;
}
.swiper-pagination-bullet.swiper-pagination-bullet-active{
	background:#b50081;
}


.mv_btn_wrap{
	border: 1px solid #efefef;
    border-radius: 10px;
    background-color: #fff;
	width:320px;
	padding: 20px 10px 30px;
    text-align: center;
    margin: 25px 0;
}
.mv_btn_wrap{
	display:flax;
	flex-direction:column;
}
.mv_btn_wrap h3{
	font-size:24px;
	font-weight:700;
	border-bottom:1px solid #dda0c2;
}
.mv_btn_wrap h3:before{
	content:"";
	display:inline-block;	
	width:26px;
	height:28px;
	background:url(/common2/img/index/common/ico_present.gif) no-repeat;
	background-size:contain;
	vertical-align:middle;
	padding-right:5px;
}
.mv_btn_wrap p{
	font-size:18px;
}
.mv_btn {
	display:inline-block;
	position:relative;
}
.mv_btn a{
	display:block;
	width:250px;
	background-color:#e75f9d;
	border:1px solid #b50081;
	border-radius:10px;
	color:#fff;
	font-size:18px;
	font-weight:700;
	text-decoration:none;
	line-height:46px;
	text-align:center;
	padding:3px 0;
}
.mv_btn a:before {
    content: "";
    position: absolute;
    margin: auto;
    top: 0;
    bottom: 0;
    right: 15px;
    width: 20px;
    height: 20px;
    border-radius: 50%;
    background-color: #fff;
}
.mv_btn a:after {
    content: "";
    position: absolute;
    margin: auto;
    top: 0;
    bottom: 0;
    right: 21px;
    width: 8px;
    height: 8px;
    border-top: 3px solid #e75f9d;
    border-right: 3px solid #e75f9d;
    transform: rotate(45deg);
}

.mv_btn a:hover{
	opacity:1.0;
	background-color:#f15599;
	color:#fff;
	transition:background-color 1s linear;
}
/* consul */
.consul_area{
	position:relative;
	width:1100px;
	height:500px;
	margin:auto;
}
.consul_wrap{
	position:absolute;
	top:-60px;
	left:100px;
	height:440px;
	border:1px solid #efefef;
	border-radius:10px;
	background-color:#fff;
	text-align:center;
	padding:20px;
	z-index:15;
}
.consul_panel{
	display:flex;
	justify-content:center;
	margin:20px 0 0;
}
.consul_panel li{
	display:block;
	cursor:pointer;
	width:250px;
	height:250px;
	border:1px solid #efefef;
	margin:0 20px;
	border-radius:10px;
	box-shadow: 0 5px 5px 0 rgba(194,194,194,.5);
}
.consul_panel li a{
	display:inline-block;
	text-decoration:none;
	color:#5e2f16;
	font-size:18px;
	font-weight:700;
	padding:10px;
}
.consul_panel li a:hover{
	background-color:#fcf3fc;
}
.consul_btn_net{
	display:flex;
	justify-content:flex-end;
}
.consul_btn_net a{
	display:block;
	background-color:#005ab6;
	border-radius:10px;
	width:300px;
	color:#fff;
	margin:30px 0 0;
	font-size:16px;
	font-weight:700;
	padding:10px;
	text-decoration:none;
	position:relative;
}
.consul_btn_net a:before{
	content:"";
	position:absolute;
	margin:auto;
	top:0;
	bottom:0;
	    right: 15px;
	width:20px;
	height:20px;
	border-radius:50%;
	background-color:#fff;
	
}
.consul_btn_net a:after{
	content:"";
	position:absolute;
	margin:auto;
	top:0;
	bottom:0;
	right: 21px;
	width:8px;
	height:8px;
	border-top:3px solid #005ab6;
	border-right:3px solid #005ab6;
	transform:rotate(45deg);
	
}	
/* お知らせ */
.info_wrap{
	width:940px;
	margin:0 auto 20px;
	border:2px solid #5e2f16;
	padding:5px 10px;
	font-size:16px;
}
.info_wrap li{
	border-bottom:2px dotted #5e2f16;
	padding:5px 0;
}
.info_wrap li:last-child{
	border:none;
}
.info_wrap li p{
	padding:0 10px;
}
.info_btn{
	text-align:right;
	padding:0 45px 0 0;
}
.info_btn p a{
	position:relative;
	color:#5e2f16;
	font-size:16px;
}
.info_btn p a:before{
	content:"";
	position:absolute;
	margin:auto;
	top:0;
	bottom:0;
	right: -25px;
	width:20px;
	height:20px;
	border-radius:50%;
	background-color:#5e2f16;
}
.info_btn p a:after{
	content:"";
	position:absolute;
	margin:auto;
	top:0;
	bottom:0;
	right: -19px;
	width:6px;
	height:6px;
	border-top:3px solid #fff;
	border-right:3px solid #fff;
	transform:rotate(45deg);
	
}
