/* 申込みの流れ */
.step_img{
	background: url(/assets/frt/img/step_left.png) right no-repeat;
}	
.step_body{
	border-bottom:1px solid #ccc;
}
.step_body .step_left,.step_midd, .step_right{
    height: 435px;
}
.step_body .step_left {
    float: left;
    width: 49.8%;
    padding: 29px;
    background: url(../img/step_left.png) right no-repeat;
    background-size: auto 435px;
    box-sizing: border-box;
}
.step_left,.step_right {
	 width:50%;
}
.step_right{
	float:left;
	font-size:18px;
}
.step_midd{
	float:left;
	display:flex;
	flex-direction:column;
	width:90px;
	background-color:#f1a7d0;
}
.step_midd li{
	height:146px;
	color:#fff;
