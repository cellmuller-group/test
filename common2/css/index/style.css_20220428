}
.step_midd li span{
	font-size:2.0em;
	font-weight:700;
}
.step_midd p{
	position: absolute;
    top: 20%;
    transform: translateY(-20%);
    left: 50%;
    transform: translateX(-50%);
}
.step_right ul{
	border-bottom:1px solid #ccc;
	height:146px;
}
.step_ttl{
	background-color:#f5d6e7;
