  .top_bnrs_bar{
    width: 100%;
    padding:10px 0;
    background:#FCF3FC;
    text-align: center;
  }
  .top_bnrs_bar li{
    display: inline-block;
    vertical-align: middle;
    width:300px;
    border:2px solid #fff;
    border-radius: 5px;
    background: #fff;
    position: relative;
  }
  .top_bnrs_bar li a{
    display: block;
    border:2px solid #ac1d7a;
    border-radius: 5px;
    text-align: left;
    font-size:14px;
    color:#ac1d7a;
    position: relative;
    text-decoration: none;
  }
  .top_bnrs_bar li a::after{
    content:"";
    display: block;
    width:12px;
    height: 12px;
    border-right:2px solid #ac1d7a;
    border-bottom:2px solid #ac1d7a;
    position: absolute;
    margin: auto;
    right:12px;
    top:0;
    bottom:0;
    transform:rotate(-45deg);
  }
  .top_bnr01{
    padding: 25px 20px;
  }
  .top_bnr02{
    padding: 15px 20px;
  }
  .top_bnrs_bar li a .break{
    display: none;
  }
  .close_top_bnrs_bar{
    width:24px;
    height: 24px;
    text-align: center;
    position: absolute;
    top:-10px;
    left:-40px;
    background:#fff;
    border-radius: 50%;
    box-shadow: -1px 1px 3px #999;
    font-size:20px;
    line-height: 1;
    cursor: pointer;
  }
